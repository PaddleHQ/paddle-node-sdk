{
  "name": "@paddle/paddle-node-sdk",
  "version": "1.9.1",
  "description": "A Node.js SDK that you can use to integrate Paddle Billing with applications written in server-side JavaScript.",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "bun run --watch examples/index.ts",
    "build": "tsup",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "prebuild": "bun ./scripts/update-env-vars.ts",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --check ./src --write",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --ext .ts,.tsx ./src --fix",
    "release:beta": "yarn version --prerelease --preid beta --no-git-tag-version --no-commit-hooks",
    "publish:beta": "yarn publish --tag beta --access public",
    "publish:latest": "yarn publish --access public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "Paddle",
    "SDK"
  ],
  "author": "paddle.com",
  "license": "Apache-2.0",
  "homepage": "https://developer.paddle.com/api-reference/overview",
  "devDependencies": {
    "@types/bun": "^1.1.11",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.6.0",
    "@types/node-fetch": "^2.6.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^8.9.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.0.1",
    "prettier": "^3.0.3",
    "tsup": "^8.3.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0"
  }
}
